            ZoomVolumes
            [
                [1, 40%]
				[1000, 10%]
            ]
            ZoomVolumeVelocityUp = 0.5
            ZoomVolumeVelocityDown = 0.5
		}
	]
	NextTracks
	[
		&~/Track1_1
		&~/Track1_2
	]
}
