Type = FSM
DebugName = "Cluster 8"
MaxConsecutivePlays = 1
IntroTracks
[
	&~/Track8_1
]

FadeOutAfter = 21*60 
FadeOutDuration = 5
FadeOutCondition = "? battle == False and wanted == False"
FadeOutOnSimPop = true
SilenceAfterFadeOut = 2

Track8_1
{
	Type = Layers
	DebugName = "Cluster8.1"
	MaxConsecutivePlays = 3
	Layers
	[
		{
			Type = File
			File = "SW_Cluster8.3_Main.music"
			DynamicVolumeVelocity = 0
            ZoomVolumes
            [
                [1, 40%]
				[1000, 10%]
            ]
            //ZoomVolumeVelocityUp = 0.1
            //ZoomVolumeVelocityDown = 0.1
		}
	]
	NextTracks
	[
		&~/Track8_1
		&~/Track8_2
	]
}

