Type = FSM
DebugName = "Cluster 5"
MaxConsecutivePlays = 1
IntroTracks
[
	&~/Track5_Intro
]

FadeOutAfter = 18*60
FadeOutDuration = 15
FadeOutCondition = "? battle == False and wanted == False"
FadeOutOnSimPop = true
SilenceAfterFadeOut = 2

Track5_Intro
{
	Type = Layers
	DebugName = "Cluster 5 intro"
	Layers
	[
		{
			Type = File
			File = "SW_galaxy_9.music"

			DynamicVolumeVelocity = 0
            ZoomVolumes
            [
                [1, 40%]
				[1000, 10%]
            ]
            //ZoomVolumeVelocityUp = 4
            //ZoomVolumeVelocityDown = 1
        }
	]
	NextTracks
	[
		&~/Track5_1
	]
}

